(def (fib n) (cond [(<= n 1) 1]
             else (+ (fib (- n 1)) (fib (- n 2)))))


(def (square n) (cond [(<= n 0) 0]
                else (+ (- (* 2 n) 1) (square (- n 1)))))

(def (square/acc n acc) (cond [(<= n 0) acc]
                         else (square/acc (- n 1) (+ (- (* 2 n) 1) acc))))

(def (square-2 n) (square/acc n 0))

